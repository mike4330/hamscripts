#!/usr/bin/gnuplot 

set title 'Unique CQ callsigns'

set timefmt "%s"
set format x "%D"
set xdata time

set key off

set style line 5 lt rgb "blue" lw 2 pt 6


set autoscale x

set xtics font ", 8"

 set xlabel 'time'
 set ylabel 'unique'
 set grid
 set term pngcairo size 900, 400
 set output '/var/www/K5ROE/cqers2.png'
set datafile separator ","
 plot '/home/mike/.local/share/WSJT-X/cqers.csv' using 1:2 with lines ls 5
